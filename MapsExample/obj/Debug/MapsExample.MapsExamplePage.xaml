<?xml version="1.0" encoding="utf-8"?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms" xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" xmlns:local="clr-namespace:MapsExample" x:Class="MapsExample.MapsExamplePage">
	<local:MapPage />
	<NavigationPage Title="Lost" >
		<NavigationPage.Padding>
			<OnPlatform x:TypeArguments="Thickness" iOS="0, 20, 0, 0" Android="0, 0, 0, 0">
			</OnPlatform>
		</NavigationPage.Padding>
		<NavigationPage.Icon>
			<OnPlatform x:TypeArguments="FileImageSource" iOS="list.png" Android="list.png">
			</OnPlatform>
		</NavigationPage.Icon>
		<x:Arguments>
			<local:LostPage />
		</x:Arguments>
	</NavigationPage>
	<local:SettingsPage />
</TabbedPage>